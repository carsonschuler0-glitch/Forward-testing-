{
  "name": "polymarket-trading-tracker",
  "version": "1.0.0",
  "description": "Bot to track high-profit traders making large trades on Polymarket",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/webServer.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "backtest": "ts-node src/backtest.ts",
    "forward-test": "ts-node src/forwardTest.ts",
    "forward-test:demo": "ts-node src/forwardTestDemo.ts",
    "web": "ts-node src/webServer.ts",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "polymarket",
    "trading",
    "bot",
    "monitoring"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@polymarket/clob-client": "^5.1.3",
    "@types/pg": "^8.16.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.64.0",
    "pg": "^8.16.3",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-telegram-bot-api": "^0.64.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
